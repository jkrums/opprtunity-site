<h3><a href="<%= @user.linked_in %>" target="_blank"><%= image_tag @user.image, :class => 'thumbnail', :style => 'margin-left:10px' %></a></h3>
<ul class="nav nav-list">
  <p><strong><%= @user.first_name %> <%= @user.last_name %></strong></p>
  <p><%= @user.location %> (<%= @user.zip_code %>) </p>
  <% unless @user.linked_in.blank? %><li><%= link_to '<i class="icon-linkedin-sign"></i>LinkedIn'.html_safe, @user.linked_in %></li><% end %>
  <% unless @user.google_plus.blank? %><li><%= link_to '<i class="icon-google-plus-sign"></i>Google+'.html_safe, @user.google_plus %></li><% end %>
  <% unless @user.company_url.blank? %><li><%= link_to '<i class="icon-bookmark"></i>Company'.html_safe, @user.company_url %></li><% end %>
  <% unless @user.phone.blank? %><li><span><i class="icon-phone-sign"></i><%= @user.phone %></span></li><% end %>
  <% unless @user.skype.blank? %><li><a href="callto:<%= @user.skype %>"><i class="icon-cloud"></i><%= @user.skype %></a></li><% end %>

</ul>
